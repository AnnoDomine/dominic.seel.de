#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/@module-federation+cli@0.14.0_typescript@5.7.3/node_modules/@module-federation/cli/bin/node_modules:/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/@module-federation+cli@0.14.0_typescript@5.7.3/node_modules/@module-federation/cli/node_modules:/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/@module-federation+cli@0.14.0_typescript@5.7.3/node_modules/@module-federation/node_modules:/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/@module-federation+cli@0.14.0_typescript@5.7.3/node_modules:/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/@module-federation+cli@0.14.0_typescript@5.7.3/node_modules/@module-federation/cli/bin/node_modules:/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/@module-federation+cli@0.14.0_typescript@5.7.3/node_modules/@module-federation/cli/node_modules:/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/@module-federation+cli@0.14.0_typescript@5.7.3/node_modules/@module-federation/node_modules:/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/@module-federation+cli@0.14.0_typescript@5.7.3/node_modules:/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@module-federation/cli/bin/mf.js" "$@"
else
  exec node  "$basedir/../@module-federation/cli/bin/mf.js" "$@"
fi
