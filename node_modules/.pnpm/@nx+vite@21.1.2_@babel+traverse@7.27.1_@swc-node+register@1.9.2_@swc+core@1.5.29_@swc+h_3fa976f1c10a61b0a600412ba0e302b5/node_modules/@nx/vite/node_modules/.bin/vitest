#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/vitest@3.1.4_@types+node@20.17.50_@vitest+ui@3.1.4_jiti@2.4.2_jsdom@22.1.0_terser@5.39.2_yaml@2.8.0/node_modules/vitest/node_modules:/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/vitest@3.1.4_@types+node@20.17.50_@vitest+ui@3.1.4_jiti@2.4.2_jsdom@22.1.0_terser@5.39.2_yaml@2.8.0/node_modules:/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/vitest@3.1.4_@types+node@20.17.50_@vitest+ui@3.1.4_jiti@2.4.2_jsdom@22.1.0_terser@5.39.2_yaml@2.8.0/node_modules/vitest/node_modules:/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/vitest@3.1.4_@types+node@20.17.50_@vitest+ui@3.1.4_jiti@2.4.2_jsdom@22.1.0_terser@5.39.2_yaml@2.8.0/node_modules:/home/hackmnin/private/github/dominic.seel.de/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitest@3.1.4_@types+node@20.17.50_@vitest+ui@3.1.4_jiti@2.4.2_jsdom@22.1.0_terser@5.39.2_yaml@2.8.0/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vitest@3.1.4_@types+node@20.17.50_@vitest+ui@3.1.4_jiti@2.4.2_jsdom@22.1.0_terser@5.39.2_yaml@2.8.0/node_modules/vitest/vitest.mjs" "$@"
fi
